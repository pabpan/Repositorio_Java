<?xml version="1.0" encoding="UTF-8"?>
<!-- MOSTRAR POR CONSOLA LA HORA DEL ERROR, UTILIZA LA CLASE LOGGER -->
<Configuration status="INFO">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss} [%c] %-5level %logger(36) - %msg%n"/>           
        </Console>      
    </Appenders>
    <Loggers>
        <Root level="debug">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>
